{
  "name": "mathsys",
  "version": "0.10.2",
  "description": "The Domain Specific Language for math.",
  "main": "typescript/index.js",
  "scripts": {
    "build/py": "python3 -m build",
    "build/ts": "babel typescript/ --extensions \".ts\" --watch --out-dir typescript/ --out-file-extension .js",
    "install/py": "pip install -e . --break-system-packages",
    "test/py": "python3 -m unittest python.testing",
    "upload/py": "twine upload dist/*",
    "upload/ts": "npm publish",
    "compile/unix/x86/64": "cd python/mathsys/bin/unix/x86/64 && nasm -f elf64 all.asm -o all.o",
    "compile/web": "cd python/mathsys/bin/web && wat2wasm --enable-annotations all.wat -r -o all.wasm && wasm-ld -flavor wasm -r all.wasm -o all.o && rm all.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abscissa-math/mathsys.git"
  },
  "keywords": [
    "dsl",
    "math",
    "mathsys",
    "compiler",
    "language"
  ],
  "author": "alejandro-vaz",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/abscissa-math/mathsys/issues"
  },
  "homepage": "https://github.com/abscissa-math/mathsys#readme",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1"
  }
}
