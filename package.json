{
  "name": "mathsys",
  "version": "6.0.0",
  "type": "module",
  "main": "./mathsys/v5/index.js",
  "description": "The Domain Specific Language for math.",
  "scripts": {
    "build/ts": "babel mathsys/dev/ --extensions \".ts\" --out-dir mathsys/dev/ --out-file-extension .js && nearleyc mathsys/dev/parser/syntax.ne -o mathsys/dev/parser/syntax.cjs",
    "install/py": "python3.14 -m build && pip install -e . --break-system-packages",
    "upload/py": "twine upload dist/*",
    "upload/ts": "npm publish",
    "compile": "bash ./mathsys/dev/bin/unix-x86-64.sh && bash ./mathsys/dev/bin/wasm.sh && ./mathsys/dev/bin/darwin.sh",
    "perf/stat": "perf stat ./file",
    "perf/record": "perf record -g ./file && perf report",
    "perf/trace": "perf trace ./file"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abscissa-math/mathsys.git"
  },
  "keywords": [
    "dsl",
    "math",
    "mathsys",
    "compiler",
    "language"
  ],
  "author": "alejandro-vaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abscissa-math/mathsys/issues"
  },
  "homepage": "https://github.com/abscissa-math/mathsys#readme",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/nearley": "^2.11.5",
    "@types/node": "^25.0.9"
  },
  "dependencies": {
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "pako": "^2.1.0"
  }
}
